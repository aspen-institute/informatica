          "application/json"
        ],
        "parameters": [
          {
            "name": "formId",
            "in": "query",
            "description": "If present will restrict which attributes can be created to only the attributes added to that form. This includes RainFocus Attributes/custom fields and the attendee's demographic information.",
            "required": false,
            "type": "string"
          },
          {
            "name": "apiProfile",
            "in": "header",
            "description": "API Profile Id used to authenticate to specific event and its associated data",
            "required": true,
            "type": "string"
          },
          {
            "name": "Request body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/postBody"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Success",
            "schema": {
              "$ref": "#/definitions/responseBody"
            }
          }
        }
      }
    }
